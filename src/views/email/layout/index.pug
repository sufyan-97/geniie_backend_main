doctype html
html
    head
        meta(charset='UTF-8')
        meta(http-equiv='X-UA-Compatible' content='IE=edge')
        meta(name='viewport' content='width=device-width, initial-scale=1.0')
        title #{title}

        include ./beforeStyle.pug
        block contentStyle
        include ./afterStyle.pug
    body
        .container
            include ./header.pug
            .content
                block section 
            include ./footer.pug
